    "def convert_to_base(n, base):\n",
    "    s = \"\"\n",
    "    while True:\n",
    "        if n < base:\n",
    "            s = str(n) + s\n",
    "            break\n",
    "        else:\n",
    "            d = n % base\n",
    "            n = n // base\n",
    "            s = str(d) + s\n",
    "    return s\n",
    "\n",
    "convert_to_base(190,9)\n",
    "# 100//9"
   ]
  }
